@model Class_Room.Models.User
@{
    ViewBag.Title = "Profile";
}

<html>
<body>
    <h2>Profile</h2>
    <hr />
    <table class="table" id="tblUser" style="margin-top:5px">
        <thead class="bg-info text-white">
            <tr>
                <th>LastName</th>
                <th>FirstMidName</th>
                <th>EnrollmentDate</th>
                <th>Type</th>
                <th>Profile</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>


    <script src="~/Scripts/jquery-3.3.1.js"></script>
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Content/Script/User/ProfileScript.js"></script>
    <link href="~/Content/Style/StyleSheet.css" rel="stylesheet" />
</body>
</html>

